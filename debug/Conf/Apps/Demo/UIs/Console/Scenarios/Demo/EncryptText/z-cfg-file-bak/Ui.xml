		</UiItem><UiItem>
			<Name>DelTxtFile</Name><TypeName>SubTransaction</TypeName>
			
			
			
			
			
			
			<DisplayName/>			
			<Value>DeleteFile;%AppTempDir%\#FileName.v#.txt</Value><ValidationRules/>
			
			
			
						
			
		</UiItem>
		
