		</UiItem><UiItem>
			<Name>Write1</Name><TypeName>Write</TypeName>
			
			
			
			
			
			
			<DisplayName> (加密算法: Md5)</DisplayName>			
			<Value/><ValidationRules/>
			
			
			
						
			
		</UiItem><UiItem>
			<Name>NewLine1</Name><TypeName>NewLine</TypeName>
			
			
			
			
			
			
			<DisplayName/>			
			<Value/><ValidationRules/>
			
			
			
						
			
		</UiItem><UiItem>
			<Name>Write2</Name><TypeName>Write</TypeName>
			
			
			
			
			
			
