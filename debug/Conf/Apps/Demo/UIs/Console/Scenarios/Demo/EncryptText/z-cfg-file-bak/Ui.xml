		</UiItem><UiItem>
			<Name>Crun</Name><TypeName>Transaction</TypeName>
			
			
			
			
			
			
			<DisplayName/>			
			<Value>Crun@=GetBool;IfElse;$Confirm.v$,Equal,y@$Xrun$</Value><ValidationRules/>
			
			
			
						
			
		</UiItem><UiItem>
			<Name>WriteLine</Name><TypeName>Transaction</TypeName>
			
			
			
			
			
			
			<DisplayName/>			
