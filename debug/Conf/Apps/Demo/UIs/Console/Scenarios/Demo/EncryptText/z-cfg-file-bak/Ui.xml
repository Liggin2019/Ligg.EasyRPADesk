			<Value/><ValidationRules/>
			
			
			
						
			
		</UiItem><UiItem>
			<Name>Confirm</Name><TypeName>ReadKey</TypeName>
			
			
			
			
			
			
			<DisplayName>y表示同意</DisplayName>			
